<p id="notice"><%= notice %></p>

<h1>V2 Jurisdictions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v2_jurisdictions.each do |v2_jurisdiction| %>
      <tr>
        <td><%= v2_jurisdiction.name %></td>
        <td><%= link_to 'Show', v2_jurisdiction %></td>
        <td><%= link_to 'Edit', edit_v2_jurisdiction_path(v2_jurisdiction) %></td>
        <td><%= link_to 'Destroy', v2_jurisdiction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V2 Jurisdiction', new_v2_jurisdiction_path %>
